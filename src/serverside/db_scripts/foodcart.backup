CREATE FUNCTION store_role(par_rolename character varying) RETURNS text
    LANGUAGE plpgsql
    AS $$
		DECLARE
			loc_rname TEXT;
			loc_res TEXT;
		BEGIN
			SELECT INTO loc_rname rolename FROM Roles  WHERE rolename = par_rolename;

			IF loc_rname ISNULL
